# Crie um arquivo .gitignore
echo "# Python
__pycache__/
*.py[cod]
*$py.class
*.egg-info/
.dist-info/
.build/
.eggs/
.venv/
venv/
ENV/
env/

# IDE/OS
.vscode/
.idea/
.DS_Store
Thumbs.db

# Streamlit
.streamlit/secrets.toml
.streamlit/logs/

# Dados do app (não versionar uploads e saves locais)
data/
!data/.gitkeep

# Cache e testes
.pytest_cache/
.mypy_cache/
.coverage
htmlcov/

# Arquivos temporários
*.log
*.tmp
" > .gitignore